  <DataSets>
    <DataSet Name="BalanceDataSet">
      <Query>
        <DataSourceName>TAL_Distributors</DataSourceName>
        <CommandText>SELECT RTRIM([CUSTOMER_NAME]) AS CustomerName, [CREDIT_LIMIT], [BALANCE], [CREDIT_LIMIT] - [BALANCE] AS AvailableBalance, RTRIM([LAST_NAME]) + ', ' + RTRIM([FIRST_NAME]) AS RepName
FROM [TAL_CUSTOMER] c JOIN [TAL_REP] r ON c.[REP_NUM] = r.[REP_NUM]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
